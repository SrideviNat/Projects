<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adrianna.fitnesspredictionapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:userName=".LocalLogin">
            <intent-filter>
                <action android:userName="android.intent.action.MAIN" />

                <category android:userName="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:userName=".Registration" />
        <activity
            android:userName=".HomePage"
            android:label="@string/title_activity_home_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Registration" />
        <activity android:name=".Fitness" />
        <activity android:name=".Weather" />
        <activity android:name=".Prediction"></activity>
    </application>

</manifest>